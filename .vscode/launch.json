  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug unittests",
      "request": "launch",
      "type": "cppvsdbg",
      "program": "gpgmm_unittests",
      "args": [
        "--gtest_catch_exceptions=0",
        "--gtest_filter=*"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/out/Debug",
      "environment": [],
      "console": "integratedTerminal"
    },
    {
      "name": "Debug end2end tests",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "gpgmm_end2end_tests",
      "args": [
        "--gtest_catch_exceptions=0",
        "--gtest_filter=*"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/out/Debug",
      "environment": [],
      "console": "integratedTerminal"
    },
    {
      "name": "Debug capture replay tests",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "gpgmm_capture_replay_tests",
      "args": [
        "--gtest_catch_exceptions=0",
        "--gtest_filter=*"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/out/Debug",
      "environment": [],
      "console": "integratedTerminal"
    },
    {
      "name": "Debug perf tests",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "gpgmm_perftests",
      "args": [
        "--benchmark_filter="
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/out/Debug",
      "environment": [],
      "console": "integratedTerminal"
    },
  ]
}
