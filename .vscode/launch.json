  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug unittests",
      "request": "launch",
      "type": "cppvsdbg",
      "windows": {
        "program": "gpgmm_unittests.exe",
      },
      "args": [
        "--gtest_filter=*"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/out/Debug",
      "environment": [],
      "externalConsole": true
    },
    {
      "name": "Debug end2end",
      "type": "cppvsdbg",
      "request": "launch",
      "windows": {
        "program": "gpgmm_end2end_tests.exe",
      },
      "args": [
        "--check-device-leaks",
        "--gtest_catch_exceptions=0",
        "--gtest_filter=*"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/out/Debug",
      "environment": [],
      "externalConsole": true
    },
    {
      "name": "Debug capture replay tests",
      "type": "cppvsdbg",
      "request": "launch",
      "windows": {
        "program": "gpgmm_capture_replay_tests.exe",
      },
      "args": [
        "--check-device-leaks",
        "--gtest_catch_exceptions=0",
        "--gtest_filter=*"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/out/Debug",
      "environment": [],
      "externalConsole": true
    },
    {
      "name": "Debug WebNN-GPGMM end2end",
      "type": "cppvsdbg",
      "request": "launch",
      "windows": {
        "program": "webnn_end2end_tests.exe",
      },
      "args": [
        "-d gpu",
        "--gtest_catch_exceptions=0",
        "--gtest_filter=*"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/out/Debug",
      "environment": [],
      "externalConsole": true
    },
    {
      "name": "Debug Dawn-GPGMM end2end",
      "type": "cppvsdbg",
      "request": "launch",
      "windows": {
        "program": "dawn_end2end_tests.exe",
      },
      "args": [
        "--gtest_catch_exceptions=0",
        "--backend=d3d12",
        "--enable-backend-validation",
        "--exclusive-device-type-preference=discrete,integrated,cpu",
        "--gtest_filter=*"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/out/Debug",
      "environment": [],
      "externalConsole": true
    },
  ]
}
